{
  "name": "can-dom-data",
  "version": "1.0.1",
  "description": "A key/value store (based on WeakMap) for associating data with DOM nodes.",
  "homepage": "https://canjs.com/doc/can-dom-data.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/canjs/can-dom-data.git"
  },
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "url": "https://www.bitovi.com/"
  },
  "scripts": {
    "detect-cycle": "detect-cyclic-packages",
    "jshint": "jshint ./*.js --config",
    "preversion": "npm test",
    "postpublish": "git push --tags && git push",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish",
    "test": "npm run detect-cycle && npm run jshint && npm run testee",
    "testee": "testee test/test.html --browsers firefox",
    "release:pre": "npm version prerelease && npm publish"
  },
  "main": "can-dom-data.js",
  "keywords": [
    "canjs",
    "data",
    "dom"
  ],
  "steal": {
    "main": "can-dom-data",
    "npmIgnore": [
      "steal-tools",
      "testee"
    ]
  },
  "dependencies": {
    "can-dom-mutate": "^1.0.0",
    "can-namespace": "1.0.0"
  },
  "devDependencies": {
    "detect-cyclic-packages": "^1.1.0",
    "jshint": "^2.9.1",
    "steal": "^1.5.13",
    "steal-qunit": "^1.0.1",
    "testee": "^0.9.0"
  },
  "license": "MIT"
}
